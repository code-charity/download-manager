{
	"manifest_version": 2,
	"name": "Download Manager",
	"version": "1.0",
	"default_locale": "en",
	"icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
	"background": {
		"scripts": [
			"config/filetype.js",
			"background.js"
		]
	},
    "browser_action": {
        "default_popup": "index.html"
    },
    "options_page": "options.html",
	"permissions": [
		"storage",
		"downloads",
		"downloads.shelf"
	]
}
